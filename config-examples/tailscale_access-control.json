        { // LUDUS wireguard
			"action": "accept",
			"src":    ["group:ludus-users"],
			"proto":  "udp",
			"dst":    ["tag:ludus:51820"],
		},
		{ // LUDUS access
			"action": "accept",
			"src":    ["group:ludus-users"],
			"proto":  "tcp",
			"dst":    ["tag:ludus:8006,8080"],
		},
		{ // TCM RANGE access
			"action": "accept",
			"src":    ["group:adm", "tag:tcm", "tag:adm"],
			"dst":    ["tag:tcm:*"],
		},
		{ // MAM RANGE access
			"action": "accept",
			"src":    ["group:mam", "group:adm"],
			"dst":    ["tag:mam:*"],
		},
	],
	"tagOwners": { // destination assets
		"tag:ludus": ["autogroup:owner"],
